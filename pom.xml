<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.zxz</groupId>
  <artifactId>ssm_templet</artifactId>
  <packaging>war</packaging>
  <version>0.0.1-SNAPSHOT</version>
  <name>ssm_templet Maven Webapp</name>
  <url>http://maven.apache.org</url>
  
	<properties>
        <!-- spring版本号 -->
        <spring.version>4.3.10.RELEASE</spring.version>

        <!-- mybatis版本号 -->
        <mybatis.version>3.2.2</mybatis.version>
        <mybatis-connector-java.version>5.1.34</mybatis-connector-java.version>
        <mybatis-spring.version>1.2.0</mybatis-spring.version>

        <javaee-api.version>7.0</javaee-api.version>
        <jstl.version>1.2</jstl.version>
        <aspectjweaver.version>1.6.9</aspectjweaver.version>
        <fastjson.version>1.2.37</fastjson.version>
        <jackson-mapper-asl.version>1.9.13</jackson-mapper-asl.version>
        <commons-fileupload.version>1.2.2</commons-fileupload.version>
        <commons-io.version>2.4</commons-io.version>
        <!-- log4j日志文件管理包版本 -->
        <slf4j.version>1.8.0-alpha2</slf4j.version>
        <log4j.version>1.2.17</log4j.version>
